<div class="wrapper_large">
  <%= form_for @user do |f| %>

    <%= f.text_field :username, {placeholder: 'Username'} %>

    <%= f.password_field :password, {placeholder: 'Password'} %>

    <%= f.text_field :fname, {placeholder: 'First'} %>

    <%= f.text_field :lname, {placeholder: 'Last Name'} %>

    <%= f.text_field :city, {placeholder: 'City'} %>

    <%= f.text_field :dob, {placeholder: 'YYYY/MM/DD'} %>
    
    <%= f.submit :create, id:"submit_button" %>
    
    <%= f.file_field :avatar %>
    
    
  <% end %>
</div>


