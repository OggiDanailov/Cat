<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username, {placeholder: 'Username'} %>
  <%= f.label :password %>
  <%= f.password_field :password, {placeholder: 'Password'} %>
  <%= f.label :fname %>
  <%= f.text_field :fname, {placeholder: 'First'} %>
  <%= f.label :lname %>
  <%= f.text_field :lname, {placeholder: 'Last Name'} %>
  <%= f.label :city %>
  <%= f.text_field :city, {placeholder: 'City'} %>
  <%= f.label :dob %>
  <%= f.text_field :dob, {placeholder: 'Birthday'} %>
  <%= f.submit :create %>
<% end %>

